  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "patch-package",
    "deploy": "ng deploy --base-href=https://parloti.github.io/ipa-quiz/",
    "\n# ------------- LINTING ------------- scripts:": "",
    "ng:lint": "ng lint",
    "lint": "npm run htmlhint && npm run eslint && npm run prettier && npm run stylelint",
    "lint:fix": "npm run htmlhint && npm run eslint:fix && npm run prettier:write && npm run stylelint:fix",
    "htmlhint": "npx htmlhint src",
    "eslint": "npx eslint --max-warnings=0 \"**/*.{js,ts,html}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "npx prettier --ignore-unknown --check .",
    "prettier:write": "npm run prettier --- --write",
    "stylelint": "npx stylelint --max-warnings=0 \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
