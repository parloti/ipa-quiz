{
  "name": "ipa-quiz",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "typecheck:spec": "tsc -p tsconfig.spec.json --noEmit",
    "test": "ng test --watch=false --coverage=true",
    "tdd": "ng test --watch=true --coverage=false",
    "validate": "npm run lint && npm run typecheck:spec && npm test && npm run build",
    "postinstall": "patch-package",
    "deploy": "ng deploy --base-href=https://parloti.github.io/ipa-quiz/",
    "\n# ------------- LINTING ------------- scripts:": "",
    "ng:lint": "ng lint",
    "lint": "npm run eslint && npm run prettier && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run prettier:write && npm run stylelint:fix",
    "eslint": "npx eslint --max-warnings=0 \"**/*.{js,ts,html}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "npx prettier --ignore-unknown --check .",
    "prettier:write": "npm run prettier --- --write",
    "stylelint": "npx stylelint --max-warnings=0 \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "eslint:inspect": "npx @eslint/config-inspector"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "21.0.5",
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.6",
